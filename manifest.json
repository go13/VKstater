{
   "background": {
      "scripts": [ "jquery.js", "background.js", "charts/d3.js", "charts/dc.js", "charts/crossfilter.js" ]
   },
   "browser_action": {
      "default_icon": "icon_128.png"
   },
   "content_scripts": [ {
      "js": [ "jquery.js", "content.js" ],
      "matches": [ "https://vk.com/*", "https://api.vk.com/*","http://vk.com/*", "http://vk.com/*", "http://api.vk.com/*" ],
      "run_at": "document_end"
   } ],
   "icons": {
      "128": "icon_128.png"
   },
   "manifest_version": 2,
   "name": "Vkstater",
   "permissions": [ "activeTab", "tabs", "storage", "https://api.vk.com/*" ],
   "version": "2.0",
   "web_accessible_resources": [ "*.js"]
}

